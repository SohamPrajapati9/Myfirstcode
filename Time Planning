#include <iostream>
using namespace std;

int main()
{
   cout<<"kitna time hai,\nminutes me bolna ?\n";
   int freetime_in_minutes;
   cin>>freetime_in_minutes;
   
   if(freetime_in_minutes>60){
       cout<<"chalo kahi chalte hai\n";
   }
   else{
       cout<<"chup chap ghar bhago";
   }
   return 0;
}
